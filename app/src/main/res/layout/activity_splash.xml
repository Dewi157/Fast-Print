<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:background="@color/dot_light_screen1"
    android:orientation="vertical"
    tools:context="com.hikmah.dewi.fastprint.view.SplashActivity">
    <ImageView
        android:layout_width="@dimen/gambar150dp"
        android:layout_height="@dimen/gambar150dp"
        android:scaleX="1.5"
        android:scaleY="1.5"
        android:id="@+id/gambar"
        android:src="@drawable/logoku"/>
        <com.hikmah.dewi.fastprint.util.FontLatoHairline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/versi"
            android:textColor="@color/putih"/>
</LinearLayout>
